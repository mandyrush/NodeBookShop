<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length <= 0) { %>
                <h1>Nothing there!</h1>
            <% } else { %>
                <div class="centered">
                    <ul>
                        <% orders.forEach(order => { %>
                        <li>
                            <h1># <%= order._id %></h1>
                            <ul>
                                <% order.products.forEach(p => { %>
                                <li><%= p.product.title %> (<%= p.quantity %>)</li>
                                <% }); %>
                            </ul>
                        </li>
                        <% }); %>
                    </ul>
                </div>
            <% } %>
        </main>
        <%- include('../includes/end.ejs') %>